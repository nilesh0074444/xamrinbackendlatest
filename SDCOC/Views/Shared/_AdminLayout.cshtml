        <script>
            $("span.clsCurrentDate").text(moment().format('MMM DD, YYYY'));


            /* ACTIVE MENU - START */

            function PageSelection(page_name) { 
                sessionStorage.setItem("PageName", page_name);
            }

            var pagename = sessionStorage.getItem("PageName");

            if (pagename == "" || pagename == null || pagename == 'undefined') {
                $("u.page-sidebar-menu li").removeClass("active");
                $("[data-menuname='Dashboard']").addClass("active");
                //$("ul.page-sidebar-menu li[data-menuname]:eq(1)").addClass("active");
            }
            else {
                if ($("[data-menuname='" + pagename + "']").hasClass('active') == false) {//$("ul.page-sidebar-menu li.nav-item")
                    $("u.page-sidebar-menu li").removeClass("active");
                    var activepage = $("[data-menuname='" + pagename + "']");
                    activepage.addClass("active");
                }
            }

            /* ACTIVE MENU - END */

            $(document).on("click", "#newmessagesnotifcations", function () {
                var sturl = '@Url.Action("ReadMessagesnotifcations","Notifications")';
                $.ajax({
                    url: sturl,
                    type: 'GET',
                    dataType: 'json',
                    data: {
                        'ID': 0
                    },
                    success: function (data) {
                      
                    },
                    error: function (e) {
                       
                    }
                });
                $("#newmessagesnotifcations .badge.badge-default").text("0")

            });
            $(document).on("click", "#pendingnotifcations", function () {

                var sturl = '@Url.Action("Readpendingnotifcations","Notifications")';
                $.ajax({
                    url: sturl,
                    type: 'GET',
                    dataType: 'json',
                    data: {
                        'ID': 0
                    },
                    success: function (data) {

                    },
                    error: function (e) {

                    }
                });
                $("#pendingnotifcations .badge.badge-default").text("0")
            });
        </script>


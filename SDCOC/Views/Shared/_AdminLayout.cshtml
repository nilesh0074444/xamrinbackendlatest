                        <ul class="page-sidebar-menu  page-header-fixed page-sidebar-menu-light " data-keep-expanded="false" data-auto-scroll="true" data-slide-speed="200" style="padding-top: 45px">
                            <!-- DOC: To remove the sidebar toggler from the sidebar you just need to completely remove the below "sidebar-toggler-wrapper" LI element -->
                            <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
                            <li class="sidebar-toggler-wrapper hide">
                                <div class="sidebar-toggler">
                                    <span></span>
                                </div>
                            </li>
                            <!-- END SIDEBAR TOGGLER BUTTON -->
                             
                            <li class="nav-item start" data-menuname="Dashboard">
                                <a href="@Url.Action("Index", "Dashboard")" class="nav-link nav-toggle" onclick="PageSelection('Dashboard')">
                                    <i class="icon-home"></i>
                                    <span class="title">Dashboard</span>
                                   
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="NewsEvents">
                                <a href="@Url.Action("Index", "NewsEvents")" class="nav-link nav-toggle" onclick="PageSelection('NewsEvents')">
                                    <i class=" icon-volume-2"></i>
                                    <span class="title">News and Events</span>
                                    
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="Users">
                                <a href="@Url.Action("Index", "Users")" class="nav-link nav-toggle" onclick="PageSelection('Users')">
                                    <i class="icon-user"></i>
                                    <span class="title">Users</span>
                                    
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="Schedule">
                                <a href="@Url.Action("Index", "Schedule")" class="nav-link nav-toggle" onclick="PageSelection('Schedule')">
                                    <i class="icon-calendar"></i>
                                    <span class="title">Schedule</span> 
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="Donation">
                                <a href="@Url.Action("Index", "Donation")" class="nav-link nav-toggle" onclick="PageSelection('Donation')">
                                    <i class="icon-diamond"></i>
                                    <span class="title">Donation</span>
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="Connect">
                                <a href="@Url.Action("Index", "Connect")" class="nav-link nav-toggle" onclick="PageSelection('Connect')">
                                    <i class="icon-link"></i>
                                    <span class="title">Connect</span>
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="Messages">
                                <a href="@Url.Action("Index", "Messages")" class="nav-link nav-toggle" onclick="PageSelection('Messages')">
                                    <i class="icon-envelope"></i>
                                    <span class="title">Messages</span>
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="PrayerRequest">
                                <a href="@Url.Action("Index", "PrayerRequest")" class="nav-link nav-toggle" onclick="PageSelection('PrayerRequest')">
                                    <i class="icon-like"></i>
                                    <span class="title">Prayer Request</span>
                                </a> 
                            </li>
                            <li class="nav-item start" data-menuname="Settings">
                                <a href="@Url.Action("Index", "Settings")" class="nav-link nav-toggle" onclick="PageSelection('Settings')">
                                    <i class="icon-settings"></i>
                                    <span class="title">Settings</span>
                                </a> 
                            </li>
                               
                        </ul>
                        <!-- END SIDEBAR MENU -->
                        <!-- END SIDEBAR MENU -->
                    </div>
                    <!-- END SIDEBAR -->
                </div>
                <!-- END SIDEBAR -->
                <!-- BEGIN CONTENT -->
                <div class="page-content-wrapper">
                    <!-- BEGIN CONTENT BODY -->
                    <div class="page-content">
                        @RenderBody()
                    </div>
                    <!-- END CONTENT BODY -->
                </div>
                <!-- END CONTENT -->
                 
            </div>
            <!-- END CONTAINER -->
             
        </div>
